asket = () => {\n    dispatch({\n      type: \"REMOVE_FROM_BASKET\",\n      id: id,\n    });\n  };\n  return (\n    <div className=\"cobroProductos\">\n      <img className=\"cobroProductos_image\" src={image} alt=\"/\" />\n\n      <div className=\"cobroProductos_info\">\n        <p className=\"cobroProductos_title\">{title}</p>\n        <p className=\"cobroProductos_price\">\n          <small>$</small>\n          <strong>{price}</strong>\n        </p>\n        <div className=\"cobroProductos_rating\">\n          {Array(rating)\n            .fill()\n            .map((_, i) => (\n              <p>⭑</p>\n            ))}\n        </div>\n        <button onClick={removeFromBasket}>Remover del carrito</button>\n      </div>\n    </div>\n  );\n}\n\nexport default CobroProductos;\n","/Users/owner/Desktop/e-commerce/e-commerce/src/Components/Login.js",[],"/Users/owner/Desktop/e-commerce/e-commerce/src/firebase.js",[],"/Users/owner/Desktop/e-commerce/e-commerce/src/Components/Payment.js",["77"],"import React from \"react\";\nimport CobroProductos from \"./CobroProductos\";\nimport \"./Payment.css\";\nimport { useStateValue } from \"./StateProvider\";\nimport { Link } from \"react-router-dom\";\n\nfunction Payment() {\n  const [{ basket, user }, dispatch] = useStateValue();\n  return (\n    <div className=\"payment\">\n      <div className=\"payment_container\">\n        <h1>\n          Checkout (<Link to=\"/cobro\">{basket?.length} items</Link>)\n        </h1>\n      </div>\n      <div className=\"payment_section\">\n        <div className=\"payment_title\">\n          <h3>Dirección de envío</h3>\n        </div>\n        <div className=\"payment_direccion\">\n          <p>{user?.email}</p>\n          <p>Jacarandas 341</p>\n          <p>Cuernavaca, Morelos</p>\n        </div>\n      </div>\n      <div className=\"payment_section\">\n        <div className=\"payment_title\">\n          <h3>Checa articulos y envio</h3>\n        </div>\n        <div className=\"payment_items\">\